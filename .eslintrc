{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "rules": {
        "eqeqeq": "off",
        "comma-style": "off",
        "comma-dangle": "off",
        "import/prefer-default-export": "off",
        "linebreak-style": "off",
        "no-console": "off",
        "class-methods-use-this": "off"
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "parser": "babel-eslint"
}